{
    "name": "synapsestudios/api-template",
    "description": "Starting point for new projects",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/synapsestudios/synapse-base"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "synapsestudios/synapse-base": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "dev-master"
    },
    "autoload": {
        "psr-0": { "Synapse": "src/" }
    }
}
